<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
	<display-name>Archetype Created Web Application</display-name>
	<servlet>
		<servlet-name>EmpolyeeFindAllController</servlet-name>
		<display-name>EmpolyeeFindAllController</display-name>
		<description></description>
		<servlet-class>com.ciq.ems.controller.EmpolyeeFindAllController</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>EmployeeDeleteController</servlet-name>
		<display-name>EmployeeDeleteController</display-name>
		<description></description>
		<servlet-class>com.ciq.ems.controller.EmployeeDeleteController</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>EmployeeUpdateController</servlet-name>
		<display-name>EmployeeUpdateController</display-name>
		<description></description>
		<servlet-class>com.ciq.ems.controller.EmployeeUdateController</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>EmployeeSaveController</servlet-name>
		<display-name>EmployeeSaveController</display-name>
		<description></description>
		<servlet-class>com.ciq.ems.controller.EmployeeSaveController</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>EmpolyeeFindAllController</servlet-name>
		<url-pattern>/findall</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>EmployeeDeleteController</servlet-name>
		<url-pattern>/delete</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>EmployeeUpdateController</servlet-name>
		<url-pattern>/update</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>EmployeeSaveController</servlet-name>
		<url-pattern>/save</url-pattern>
	</servlet-mapping>
</web-app>
